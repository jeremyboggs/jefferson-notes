<h1>StaticPages#home</h1>
<p>Find me in app/views/static_pages/home.html.erb</p>

<div class="citation">
  <p>
  <%= @witness.author %>. <i><%= @witness.title %></i>. <%= @witness.location %>: <%= @witness.location %>,   <%= @witness.year %>.
  </p>
</div>

 <div class="pages">
  <% @witness.pages.each do |page| %>
    <div class="image-block">
      <%= link_to image_tag('http://baconmockup.com/200/200'), witness_page_path(@witness, page) %>
    </div>
  <% end %>
</div>

